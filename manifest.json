{
  "manifest_version": 3,
  "name": "ChatGPT Prompts Extension",
  "version": "1.0.0",
  "description": "Speed up your search with accuracy using the prompts manager.",
  "author": "Nitin Raturi (raturi002@gmail.com)",
  "host_permissions": [
    "https://chat.openai.com/"
  ],
  "icons": {
    "16": "./icon.png",
    "32": "./icon.png",
    "48": "./icon.png",
    "128": "./icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://chat.openai.com/chat"
      ],
      "js": [
        "./content.js"
      ],
      "css": [
        "./styles.css"
      ]
    }
  ]
}